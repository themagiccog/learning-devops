pip install fastapi uvicorn gunicorn 'databases[postgresql]' 'databases[aiosqlite]' 'databases[sqlite]'
pip install 'databases[aiosqlite]'

python -m venv env
source ./env/bin/activate

uvicorn --port 8000 --host 127.0.0.1 main:app --reload


